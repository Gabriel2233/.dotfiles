separator_block_width=15

[BATTERY]
command=./battery.sh
interval=5

[DISK]
command=df -h / | awk '/\//{ printf(" 💾 %4s/%s \n", $4, $2) }'
interval=2
color=#C9E3DB

[MEM]
command=free -h | grep Mem | awk '{ printf("Mem: 🐏 %5s/%s \n", $2, $3) }'
interval=1
color=#FEC925

[SOUND]
command=./sound.sh
interval=1
signal=1

[time]
command=date '+%D %T'
interval=5

